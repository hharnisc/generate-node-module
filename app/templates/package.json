{
  "name": "<%= name %>",
  "version": "0.0.1",
  "description": "<%= description %>",
  "main": "./lib/index.js",
  "files": [
    "lib/",
    "src/"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.2",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.7.2",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "tape": "^4.5.1",
    "nodemon": "^1.9.1"
  },
  "scripts": {
    "start": "node .",
    "build": "./node_modules/babel-cli/bin/babel.js src --out-dir lib",
    "lint": "./node_modules/eslint/bin/eslint.js src/* test/*",
    "test": "./node_modules/tape/bin/tape test/*.js -r babel-register",
    "test:watch": "./node_modules/nodemon/bin/nodemon.js -w src -w test -e js -x 'npm run lint && npm run test'",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "<%= githubUrl %>"
  },
  "keywords": ["<%= keywords %>"],
  "author": {
    "name": "<%= authorName %>",
    "email": "<%= authorEmail %>",
    "url": "<%= authorUrl %>"
  },
  "homepage": "<%= baseUrl %>"
}
